<ul>
  <% @venues.each do |venue| %>
    <li><%= venue.name %></li>
    <li><%= venue.address %></li>
    <li> <%= link_to 'View listing', venue_path(venue) %></li>
    <br>
  <% end %>
</ul>

<div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>

