<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to "/", class: "navbar-brand" do %>
  <img src="https://cdn.clipart.email/58c5b00890a38cfddb75a7eb1cfbd52a_shrink-url-png-picture-836572-shrink-url-png_512-512.png">
    <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <form class="form-inline my-2 my-lg-0">
          <input class="form-control mr-sm-2" name="query" type="search" placeholder="Search Venue" aria-label="Search" value="<%= params[:query] %>">
          <button class="btn btn-warning m-2 my-sm-0" type="submit">Search</button>
      </form>

      <% if user_signed_in? %>
        <li class="nav-item active">
          <%= link_to "Home", "/venues", class: "nav-link text-white  mx-2" %>
        </li>
        <li class="nav-item">
          <%= link_to "Messages", "#", class: "nav-link text-white  mx-2" %>
        </li>
        <li class="nav-item">
          <%= link_to "Add Venue", "venues/new", class: "nav-link text-white  mx-2" %>
        </li>
        <li class="nav-item dropdown  mx-2">
          <%= cl_image_tag "yess_b7tqty", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
            <%= link_to "Action", "#", class: "dropdown-item" %>
            <%= link_to "Another action", "#", class: "dropdown-item" %>
            <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
          </div>
        </li>
      <% else %>
        <li class="nav-item">
          <%= link_to "Login", new_user_session_path, class: "nav-link" %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
